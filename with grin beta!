         position: relative;
         margin: 0;
    }
    /* FONT COLLECTION */
     body, input, select, button, textarea {
         font-family: 'Arial', sans-serif;
    }
     .b-topic .name-date .name, .b-comment .name-date .name, .b-dialog .name-date .name, text, .highcharts-tooltip text tspan:last-child {
         font-weight: 700 !important;
    }
     @ media only screen and (min-width: 768px) {
         .about-bb_codes: before {
             font-weight: 700 !important;
        }
    }
     @media screen and (min-width: 1025px) {
         .b-entry-info .line .value {
             font-weight: 400 !important;
        }
    }
    /* ACCENT COLOR */
     .menu .unread-count, .menu:hover .unread-count, .menu .contest, .menu:hover .contest, .p-contests .b-contest_match .votes-percents .left-part, .p-contests .b-contest_match .votes-percents .left-part:hover, .b-options-floated a span.brackets, .b-options-floated .link span.brackets, .b-stats_bar.manga .bar .first, .b-stats_bar.manga .bar .second, div.compatibility > a > span.rate, div.compatibility > span.link-replacement > span.rate {
