/* AUTO=page_border */ .l-page { outline: 20px solid rgba(255, 255, 255, 0.3); margin-bottom: 20px; }
/** * Bootstrap 3 Tabs */
/* Стиль кнопок */
 [data-tab-switch] {
     display: inline-block;
     padding: 10px 15px;
     margin-right: 2px;
     margin-bottom: -1px;
     border: 1px solid transparent;
     border-radius: 4px 4px 0 0;
     color: #777;
     cursor: pointer;
}
/* Кнопка при наведении */
 [data-tab-switch]:not(.active):hover {
     background-color: #eee;
     border-color: #eee #eee #ddd;
}
/* Активная кнопка */
 [data-tab-switch].active {
     color: #fff;
     cursor: default;
     background-color: #ff0000;
     border: 1px solid #ddd;
     border-bottom-color: transparent;
}
/* Стиль табов */
 [data-tab] {
     padding: 15px;
     background-color: #EEEEEE;
     border: 1px solid #DDDDDD;
}
/* Стиль широкополосного спойлера */
 .fullwidth > .b-spoiler > label {
     display: block !important;
     padding: .5em 1em;
     background: #ff0000;
     border: 0;
     border-radius: 3px;
     color: #FAFAFA;
     font-size: 1.25em;
     line-height: 1em;
     text-overflow: ellipsis;
     white-space: nowrap;
     overflow: hidden;
}
 .fullwidth > .b-spoiler > label:active {
    /* transform: translate3d(0, 1px, 0);
    /* Анимация надавливания */
     ;
}
 .fullwidth > .b-spoiler > label::after {
     content: 'Открыть';
     float: right;
     padding: .25em .5em;
     margin: -.25em -.5em;
     border-radius: 3px;
     font-size: .7em;
     line-height: inherit;
     transition: 0.16s cubic-bezier(0.4, 0, 0.6, 1);
}
 .fullwidth > .b-spoiler > label:hover::after, .fullwidth > .b-spoiler > label[style*='none']::after {
     background: rgba(0, 0, 0, 0.2);
}
 .fullwidth > .b-spoiler > label[style*='none']::after {
     content: 'Закрыть';
    /* transform: rotate(180deg);
    /* Поворот для анимации стрелок */
     ;
}
 .fullwidth > .b-spoiler > label > span {
     display: inline-block;
     vertical-align: top;
}
 .fullwidth > .b-spoiler > .content > .before {
     display: block;
     position: relative;
     width: 100%;
     height: 2em;
     margin-top: -2em;
     margin-bottom: .5em;
     font-size: 1.25em;
     z-index: 1;
}
 .fullwidth > .b-spoiler > .content > .before::before, .fullwidth > .b-spoiler > .content > .after {
     display: none;
}
 .fullwidth > .b-spoiler > .content > .inner, .fullwidth > .b-spoiler > .content .inner-prgrph {
     border: 0;
}
 .fullwidth > .b-spoiler > .content > .inner {
     display: block;
     min-height: 0;
     padding: .5em 1em;
     margin: .5em 0;
     background: #EEEEEE;
     border: 2px solid #DDDDDD;
     border-radius: 3px;
     color: #212121;
     animation: 0.24s fadeIn cubic-bezier(0.4, 0, 0.6, 1);
}
/* Стандартная анимация появления */
 @keyframes fadeIn {
     from {
         opacity: 0;
    }
     to {
         opacity: 1;
    }
}
/* Общие настройки для табов */
 .tab {
     display: inline-block;
     padding: 0 16px;
     border-bottom: 2px solid #607D8B;
     font-size: 14px;
     line-height: 32px;
     user-select: none;
}
/* Неактивный таб */
 .tablist .tab:nth-child(2), .tablist.active .tab:nth-child(1) {
     background-color: #FAFAFA;
     color: #212121;
}
/* Активный таб */
 .tablist .tab:nth-child(1), .tablist.active .tab:nth-child(2) {
     background-color: #607D8B;
     color: #FAFAFA;
}
/* Удаляем пробел между табами */
 .tablist {
     font-size: 0;
}
/* Неактивная вкладка */
 .tab_panel, .tablist.active + .tab_panel {
     display: none;
}
/* Активная вкладка */
 .tablist + .tab_panel, .tablist.active ~ .tab_panel + .tab_panel {
     display: block;
}
